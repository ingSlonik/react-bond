{
  "name": "react-neutron",
  "version": "0.1.0",
  "description": "react-neutron is a framework for building cross-platform lightweight desktop apps with the React.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "ts-node",
    "build": "tsc src/index.tsx --outDir lib --target esnext --module commonjs --esModuleInterop true --jsx react --sourceMap true --declaration true",
    "build-example": "tsc example/window.tsx --outDir example/lib --target esnext --module commonjs --esModuleInterop true --jsx react",
    "pkg-example": "pkg --config package.json example/lib/example/window.js",
    "pkg-hello": "pkg lib/hello.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingSlonik/react-neutron.git"
  },
  "keywords": [
    "react",
    "desktop",
    "app",
    "lightweight"
  ],
  "author": "Filip Paul≈Ø",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ingSlonik/react-neutron/issues"
  },
  "homepage": "https://github.com/ingSlonik/react-neutron#readme",
  "dependencies": {
    "native-webview": "^0.3.0",
    "react": "^17.0.2",
    "react-reconciler": "^0.26.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-reconciler": "^0.26.4",
    "pkg": "^5.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "pkg": {
    "outputPath": "dist"
  }
}
